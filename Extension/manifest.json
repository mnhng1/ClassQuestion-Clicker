{
    "manifest_version": 3,
    "name": "Auto Answer Clicker",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://classquestion.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }